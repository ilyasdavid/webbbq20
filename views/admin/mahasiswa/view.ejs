                                        </div>
                                    </div>
                                    <!-- alamat -->
                                    <div class="form-group">
                                        <label for="inputAlamat" class="col-sm-2 control-label">Alamat</label>
                                        <div class="col-sm-10">
                                            <input required name="alamat" type="text" class="form-control"
                                                id="inputAlamat" placeholder="Alamat" />
                                        </div>
                                    </div>
                                    <!-- DatePicker Tanggal Lahir -->
                                    <div class="form-group">
