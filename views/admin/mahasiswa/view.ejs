<%- include ../../templates/adminlte/header-nav.ejs %>
<%- include ../../templates/adminlte/sidebar_admin.ejs %>
<!-- Full Width Column -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Lihat Mahasiswa
        </h1>
        <ol class="breadcrumb">
            <li class="active"><a href="/admin"><i class="fa fa-home"></i> Home</a></li>
            <li class="active"><a href="#"> Lihat Mahasiswa</a></li>
        </ol>
        <% if(action === "view" && alert.message != '') { %>
        <br>
        <div class="alert alert-<%= alert.status %>" style="text-align: center;">
            <strong><%= alert.message %></strong>
        </div>
        <% } %>
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-md-12">
                <% if(action === "view") { %>
                <div class="nav-tabs-custom">
                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#activity" data-toggle="tab">Lihat Mahasiswa</a></li>
                        <li><a href="#input-mahasiswa" data-toggle="tab">Input Mahasiswa</a></li>                        
                    </ul>
                    <div class="tab-content">
                        <div class="active tab-pane" id="activity">
                            <div class="box-body table-responsive no-padding">
                                <table id="tableMahasiswa" class="table table-hover">
                                    <thead>
                                        <th>No.</th>
                                        <th style="text-align: center">NPM</th>
                                        <th>Nama</th>
                                        <th style="text-align: center">Foto</th>
                                        <th style="text-align: center">Jurusan</th>
                                        <th>Kelas</th>
                                        <th>Jenis Kelamin</th>
                                        <th style="text-align: center">No. Hp</th>
                                        <th>Status</th>
                                        <th style="text-align: center">Aksi</th>
                                    </thead>
                                    <tbody>
                                        <% for (var i = 0; i < mahasiswas.length; i++) { %>
                                        <tr>
                                            <td>
                                                <%= i+1 %>
                                            </td>
                                            <td>
                                                <%= mahasiswas[i].npm %>
                                            </td>
                                            <td>
                                                <%= mahasiswas[i].nama %>
                                            </td>
                                            <td>
                                                <% if (mahasiswas[i].foto) { %>
                                                <img
                                                    src="/foto-peserta-bbq/<%= mahasiswas[i].foto %>"
                                                    height="70"
                                                    width="70"
                                                    alt="foto"
                                                />
                                                <% } else { %>
                                                    Tidak Ada
                                                <% } %>                                                
                                            </td>
                                            <td>
                                                <%= mahasiswas[i].Jurusan.namaJurusan %>
                                            </td>
                                            <td>
                                                <%= mahasiswas[i].Kela.namaKelas %>
                                            </td>
                                            <td>
                                                <%= mahasiswas[i].jenisKelamin %>
                                            </td>
                                            <td>
                                                <%= mahasiswas[i].nomorHp %>
                                            </td>
                                            <td>
                                                <%= mahasiswas[i].status %>
                                            </td>
                                            <td>
                                                <a class="btn btn-warning btn-xs"
                                                    href="/admin/mahasiswa/edit/<%= mahasiswas[i].id %>">
                                                    <span class="glyphicon glyphicon-edit"></span>
                                                    Edit
                                                </a>
                                                <button class="btn btn-danger btn-xs"
                                                    onclick="myFunction(`<%= mahasiswas[i].id %>`)"
                                                    data-id="<%= mahasiswas[i].id %>">
                                                    <span class="glyphicon glyphicon-trash"></span>
                                                    Delete
                                                </button>
                                            </td>
                                        </tr>
                                        <% } %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!-- /.tab-pane -->
                        <div class="tab-pane" id="input-mahasiswa">
